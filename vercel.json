{
  "buildCommand": "npx prisma generate && npx prisma db push --accept-data-loss && next build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "functions": {
    "app/api/**": {
      "maxDuration": 30
    }
  },
  "env": {
    "NEXT_PUBLIC_NGROK_URL": "@next_public_ngrok_url"
  }
}